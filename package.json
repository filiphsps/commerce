{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@nordcom/ecommerce",
    "version": "1.4.0",
    "private": true,
    "type": "module",
    "engines": {
        "npm": ">=8",
        "node": ">=18 <=21"
    },
    "cacheDirectories": [
        "apps/**/.next/cache"
    ],
    "prettier": "@nordcom/prettier",
    "scripts": {
        "prepare": "husky install",
        "start": "NODE_OPTIONS=\"--no-deprecation\" turbo run start",
        "dev": "NODE_OPTIONS=\"--no-deprecation\" turbo run dev",
        "build": "NODE_OPTIONS=\"--no-deprecation\" turbo run build",
        "lint": "NODE_OPTIONS=\"--no-deprecation\" turbo run lint",
        "typecheck": "NODE_OPTIONS=\"--no-deprecation\" turbo run typecheck",
        "test": "NODE_OPTIONS=\"--no-deprecation\" turbo run test",
        "slicemachine": "NODE_OPTIONS=\"--no-deprecation\" turbo run slicemachine"
    },
    "author": {
        "name": "Nordcom Group Inc.",
        "email": "opensource@nordcom.io",
        "url": "https://nordcom.io/"
    },
    "contributors": [
        {
            "name": "Filiph Siitam SandstrÃ¶m",
            "email": "filiph@nordcom.io",
            "url": "https://github.com/filiphsps/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NordcomInc/sweetsideofsweden-frontend.git"
    },
    "keywords": [
        "nextjs",
        "react"
    ],
    "bugs": {
        "url": "https://github.com/NordcomInc/sweetsideofsweden-frontend/issues"
    },
    "homepage": "https://www.sweetsideofsweden.com/",
    "browserslist": [
        ">0.5%",
        "defaults",
        "supports es6-module",
        "supports css-grid"
    ],
    "workspaces": [
        "apps/*"
    ],
    "devDependencies": {
        "@nordcom/prettier": "0.1.1",
        "@typescript-eslint/eslint-plugin": "6.9.1",
        "babel-plugin-module-resolver": "5.0.0",
        "concurrently": "8.2.2",
        "eslint": "8.52.0",
        "eslint-config-next": "14.0.1",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-import": "2.29.0",
        "eslint-plugin-jsdoc": "46.8.2",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "eslint-plugin-prettier": "5.0.1",
        "eslint-plugin-unused-imports": "3.0.0",
        "gzip-size": "7.0.0",
        "lint-staged": "15.0.2",
        "prettier": "3.0.3",
        "turbo": "1.10.16",
        "typescript": "5.2.2"
    },
    "packageManager": "npm@10.2.2",
    "nextBundleAnalysis": {
        "budget": 248400,
        "budgetPercentIncreaseRed": 20,
        "minimumChangeThreshold": 0,
        "showDetails": false,
        "buildOutputDirectory": "apps/web/.next"
    }
}
