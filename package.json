{
    "name": "candybysweden-frontend",
    "version": "1.0.0",
    "private": true,
    "cacheDirectories": [
        ".next/cache"
    ],
    "scripts": {
        "dev": "next",
        "build": "next build",
        "analyze": "ANALYZE=true npm run build",
        "start": "next start -p $PORT",
        "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
        "create-component": "node ./tools/create-component.js",
        "create-slice": "node ./tools/create-slice.js",
        "create-view": "node ./tools/create-view.js",
        "prettier": "prettier --config .prettierrc src/**/*.* --check",
        "prettier-format": "prettier --config .prettierrc src/**/*.* --write"
    },
    "dependencies": {
        "@apollo/client": "3.5.8",
        "@liholiho/react-modal-hook": "3.0.0",
        "@prismicio/client": "6.2.0",
        "@prismicio/helpers": "2.1.1",
        "axios": "0.27.2",
        "color": "4.2.0",
        "core-js": "3.21.0",
        "countries-list": "2.6.1",
        "country-code-info": "0.3.0",
        "country-code-lookup": "0.0.20",
        "cross-env": "7.0.3",
        "destyle.css": "3.0.2",
        "graphql": "16.3.0",
        "isomorphic-unfetch": "3.1.0",
        "mini-css-extract-plugin": "2.5.3",
        "moment": "2.29.1",
        "next": "12.1.6",
        "next-seo": "5.4.0",
        "next-transpile-modules": "9.0.0",
        "nprogress": "0.2.0",
        "react": "17.0.2",
        "react-alert": "7.0.3",
        "react-context-hook": "3.0.7",
        "react-dom": "17.0.2",
        "react-icons": "4.3.1",
        "react-image": "4.0.3",
        "react-image-gallery": "1.2.7",
        "react-markdown": "5.0.3",
        "react-responsive-carousel": "3.2.22",
        "react-scroll-up": "1.3.7",
        "react-select": "5.2.2",
        "react-tender": "1.0.0",
        "sass": "1.51.0",
        "styled-components": "5.3.5",
        "swr": "1.3.0",
        "typescript": "4.5.5"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "12.0.10",
        "@testing-library/jest-dom": "5.16.2",
        "@testing-library/react": "12.1.3",
        "@types/enzyme": "3.10.11",
        "@types/node": "16.11.24",
        "@types/react": "17.0.39",
        "@types/styled-components": "5.1.22",
        "enzyme": "3.11.0",
        "eslint": "8.8.0",
        "eslint-plugin-import": "2.25.4",
        "eslint-plugin-react": "7.28.0",
        "eslint-plugin-react-hooks": "4.3.0",
        "jest": "27.5.1",
        "prettier": "2.5.1",
        "webpack": "5.68.0"
    }
}
