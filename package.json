{
    "name": "sweetsideofsweden-frontend",
    "version": "1.3.0",
    "private": true,
    "prettier": "@nordcom/prettier",
    "type": "module",
    "engines": {
        "npm": ">=8",
        "node": ">=18.12"
    },
    "cacheDirectories": [
        ".next/cache"
    ],
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "postbuild": "npm run sitemap",
        "sitemap": "next-sitemap && npm run sitemap:pretty",
        "sitemap:pretty": "for i in public/*.xml; do cat $i | pretty-xml | tee $i > /dev/null; done",
        "start": "next start -p $PORT",
        "lint": "next lint",
        "test": "npm-run-all test:*",
        "test:jest": "jest --coverage",
        "watch:test": "jest --watch",
        "format": "npm-run-all prettier:format:*",
        "prettier": "prettier src/**/*.* --check",
        "prettier:format:src": "prettier src/**/*.* --write",
        "prettier:format:pages": "prettier pages/**/*.* --write",
        "prettier:format:app": "prettier app/**/*.* --write",
        "slicemachine": "start-slicemachine"
    },
    "dependencies": {
        "@apollo/client": "3.8.5",
        "@prismicio/client": "7.3.1",
        "@prismicio/helpers": "2.3.9",
        "@prismicio/next": "1.3.6",
        "@prismicio/react": "2.7.3",
        "@shopify/hydrogen-react": "2023.7.4",
        "@sweetsideofsweden/next-plugin-preval": "2.0.0",
        "@thebeyondgroup/shopify-rich-text-renderer": "1.0.0",
        "accept-language-parser": "1.5.0",
        "color": "4.2.3",
        "convert-units": "2.3.4",
        "destyle.css": "4.0.0",
        "graphql": "16.8.1",
        "i18next": "23.5.1",
        "next": "13.5.4",
        "next-i18next": "14.0.3",
        "next-seo": "6.1.0",
        "nprogress": "0.2.0",
        "react": "18.2.0",
        "react-context-hook": "github:sweet-side-of-sweden/react-context-hook",
        "react-dom": "18.2.0",
        "react-i18next": "13.3.0",
        "react-icons": "4.11.0",
        "react-slick": "0.29.0",
        "sass": "1.69.3",
        "sharp": "0.32.6",
        "slick-carousel": "1.8.1",
        "styled-components": "6.1.0",
        "swr": "2.2.4",
        "typescript": "5.2.2"
    },
    "devDependencies": {
        "@nordcom/prettier": "0.1.1",
        "@slicemachine/adapter-next": "0.3.20",
        "@testing-library/react": "14.0.0",
        "@types/accept-language-parser": "1.5.4",
        "@types/color": "3.0.4",
        "@types/convert-units": "2.3.7",
        "@types/enzyme": "3.10.14",
        "@types/node": "18.18.5",
        "@types/nprogress": "0.2.1",
        "@types/react": "18.2.28",
        "@types/styled-components": "5.1.28",
        "babel-plugin-module-resolver": "5.0.0",
        "encoding": "0.1.13",
        "enzyme": "3.11.0",
        "eslint": "8.51.0",
        "eslint-config-next": "13.5.4",
        "eslint-config-prettier": "9.0.0",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "next-sitemap": "4.2.3",
        "npm-run-all": "4.1.5",
        "prettier": "3.0.3",
        "pretty-xml": "1.2.1",
        "slice-machine-ui": "1.17.0"
    },
    "nextBundleAnalysis": {
        "budget": null,
        "budgetPercentIncreaseRed": 20,
        "minimumChangeThreshold": 0,
        "showDetails": true
    }
}
