{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@nordcom/commerce",
    "version": "2.1.0",
    "private": true,
    "type": "module",
    "cacheDirectories": [
        ".next/cache",
        "apps/**/.next/cache"
    ],
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev": "dotenv -c -e . -- turbo run dev --env-mode=loose",
        "dev:admin": "dotenv -c -e . -- turbo run dev --filter=./apps/admin --env-mode=loose",
        "dev:landing": "dotenv -c -e . -- turbo run dev --filter=./apps/landing --env-mode=loose",
        "dev:storefront": "dotenv -c -e . -- turbo run dev --filter=./apps/storefront --env-mode=loose",
        "build": "dotenv -c -e . -- turbo run build --env-mode=loose --remote-cache-timeout=60",
        "build:packages": "turbo run build --filter=./packages/* --env-mode=loose --remote-cache-timeout=60",
        "clean": "turbo run clean",
        "lint": "turbo run lint",
        "prettier": "prettier **/src/**/*.{ts,tsx} --check",
        "format": "concurrently pnpm:format:* && turbo run format && turbo run fix:lint",
        "format:eslint": "eslint --fix **/src/**/*.{ts,tsx}",
        "format:prettier": "prettier --write **/src/**/*.{ts,tsx,scss,json}",
        "typecheck": "turbo run typecheck",
        "test": "dotenv -c -e . -- vitest run --coverage",
        "slicemachine": "turbo run slicemachine --env-mode=loose"
    },
    "author": {
        "name": "Nordcom AB",
        "email": "hi@nordcom.io",
        "url": "https://nordcom.io/"
    },
    "contributors": [
        {
            "name": "Filiph Siitam Sandstr√∂m",
            "email": "filiph@nordcom.io",
            "url": "https://github.com/filiphsps/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NordcomInc/ecommerce.git"
    },
    "keywords": [
        "nextjs",
        "react"
    ],
    "bugs": {
        "url": "https://github.com/NordcomInc/ecommerce/issues"
    },
    "homepage": "https://shops.nordcom.io/",
    "dependencies": {
        "@nordcom/nordstar": "0.0.60"
    },
    "devDependencies": {
        "@nordcom/prettier": "0.1.1",
        "@types/node": "20.12.7",
        "@types/react-dom": "18.2.24",
        "@types/react": "18.2.75",
        "@typescript-eslint/eslint-plugin": "8.0.0",
        "@typescript-eslint/parser": "8.0.0",
        "@vitejs/plugin-react": "4.3.1",
        "@vitest/coverage-v8": "1.5.0",
        "babel-plugin-module-resolver": "5.0.2",
        "concurrently": "9.0.1",
        "dotenv-cli": "7.4.2",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-jsdoc": "50.2.4",
        "eslint-plugin-jsx-a11y": "6.10.0",
        "eslint-plugin-prettier": "5.2.1",
        "eslint-plugin-react-compiler": "0.0.0-experimental-7670337-20240918",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-react": "7.36.1",
        "eslint-plugin-simple-import-sort": "12.1.0",
        "eslint-plugin-unused-imports": "4.0.0",
        "eslint": "8.57.1",
        "glob": "11.0.0",
        "import-in-the-middle": "1.11.0",
        "mongoose": "8.4.0",
        "prettier-eslint": "16.3.0",
        "prettier-plugin-tailwindcss": "0.6.6",
        "prettier": "3.3.3",
        "turbo-ignore": "2.1.2",
        "turbo": "2.1.2",
        "typescript": "5.6.2",
        "vite-plugin-dts": "3.8.3",
        "vite-tsconfig-paths": "5.0.1",
        "vite": "5.4.6",
        "vitest": "1.5.0"
    },
    "optionalDependencies": {
        "@rollup/rollup-darwin-arm64": "4.18.0",
        "@rollup/rollup-linux-x64-gnu": "4.18.0"
    },
    "overrides": {
        "@typescript-eslint/typescript-estree": "8.0.0",
        "esbuild": "0.23.1",
        "psl": "npm:@filiphsandstrom/psl@2.0.0"
    },
    "packageManager": "pnpm@9.10.0"
}
