{
    "name": "sweetsideofsweden-frontend",
    "version": "1.3.0",
    "private": true,
    "type": "module",
    "engines": {
        "npm": ">=8",
        "node": ">=18.12"
    },
    "cacheDirectories": [
        ".next/cache"
    ],
    "scripts": {
        "dev": "npm run hack && next dev",
        "build": "npm run hack && next build",
        "postbuild": "npm run sitemap",
        "sitemap": "next-sitemap && npm run sitemap:pretty",
        "sitemap:pretty": "for i in public/*.xml; do cat $i | pretty-xml | tee $i > /dev/null; done",
        "start": "npm run hack && next start -p $PORT",
        "lint": "npm run hack && next lint",
        "test": "npm run hack && npm-run-all test:*",
        "test:jest": "jest --coverage",
        "watch:test": "jest --watch",
        "format": "npm-run-all prettier:format:*",
        "prettier": "prettier --config .prettierrc src/**/*.* --check",
        "prettier:format:src": "prettier --config .prettierrc src/**/*.* --write",
        "prettier:format:pages": "prettier --config .prettierrc pages/**/*.* --write",
        "prettier:format:app": "prettier --config .prettierrc app/**/*.* --write",
        "slicemachine": "npm run hack && start-slicemachine",
        "hack": "npm-run-all --sequential hack:*",
        "hack:useclient-directive": "node scripts/useclient-directive-hack.mjs"
    },
    "dependencies": {
        "@apollo/client": "3.8.1",
        "@prismicio/client": "7.1.1",
        "@prismicio/helpers": "2.3.9",
        "@prismicio/next": "1.3.4",
        "@prismicio/react": "2.7.1",
        "@sentry/nextjs": "7.64.0",
        "@shopify/hydrogen-react": "2023.7.1",
        "@sweetsideofsweden/next-plugin-preval": "2.0.0",
        "@thebeyondgroup/shopify-rich-text-renderer": "1.0.0",
        "accept-language-parser": "1.5.0",
        "color": "4.2.3",
        "convert-units": "2.3.4",
        "destyle.css": "4.0.0",
        "graphql": "16.8.0",
        "i18next": "23.4.4",
        "next": "13.4.16",
        "next-i18next": "14.0.0",
        "next-seo": "6.1.0",
        "node-html-parser": "6.1.5",
        "nprogress": "0.2.0",
        "react": "18.2.0",
        "react-context-hook": "github:sweet-side-of-sweden/react-context-hook",
        "react-dom": "18.2.0",
        "react-i18next": "13.1.2",
        "react-icons": "4.10.1",
        "react-rating-stars-component": "2.2.0",
        "react-shopify-analytics": "1.1.0",
        "react-slick": "0.29.0",
        "sass": "1.65.1",
        "sharp": "0.32.5",
        "slick-carousel": "1.8.1",
        "styled-components": "6.0.7",
        "stylis": "4.3.0",
        "swr": "2.2.1",
        "typescript": "5.1.6"
    },
    "devDependencies": {
        "@slicemachine/adapter-next": "0.3.10",
        "@testing-library/react": "14.0.0",
        "@types/accept-language-parser": "1.5.3",
        "@types/color": "3.0.3",
        "@types/convert-units": "2.3.6",
        "@types/enzyme": "3.10.13",
        "@types/node": "18.17.5",
        "@types/react": "18.2.20",
        "babel-plugin-module-resolver": "5.0.0",
        "encoding": "0.1.13",
        "enzyme": "3.11.0",
        "eslint": "8.47.0",
        "eslint-config-next": "13.4.16",
        "eslint-config-prettier": "9.0.0",
        "jest": "29.6.2",
        "jest-environment-jsdom": "29.6.2",
        "next-sitemap": "4.2.2",
        "npm-run-all": "4.1.5",
        "prettier": "3.0.2",
        "pretty-xml": "1.2.1",
        "slice-machine-ui": "1.8.0"
    }
}
