{
    "name": "candybysweden-frontend",
    "version": "1.0.0",
    "private": true,
    "cacheDirectories": [
        ".next/cache"
    ],
    "scripts": {
        "dev": "next",
        "build": "next build",
        "analyze": "ANALYZE=true npm run build",
        "start": "next start -p $PORT",
        "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
        "create-component": "node ./tools/create-component.js",
        "create-slice": "node ./tools/create-slice.js",
        "create-view": "node ./tools/create-view.js",
        "prettier": "prettier --config .prettierrc src/**/*.* --check",
        "prettier-format": "prettier --config .prettierrc src/**/*.* --write"
    },
    "dependencies": {
        "@apollo/client": "3.6.2",
        "@liholiho/react-modal-hook": "3.0.0",
        "@prismicio/client": "6.4.3",
        "@prismicio/helpers": "2.3.0",
        "axios": "0.27.2",
        "color": "4.2.3",
        "countries-list": "2.6.1",
        "country-code-info": "0.3.0",
        "country-code-lookup": "0.0.20",
        "cross-env": "7.0.3",
        "destyle.css": "3.0.2",
        "graphql": "16.5.0",
        "isomorphic-unfetch": "3.1.0",
        "mini-css-extract-plugin": "2.6.0",
        "moment": "2.29.3",
        "next": "12.1.6",
        "next-seo": "5.4.0",
        "next-transpile-modules": "9.0.0",
        "nprogress": "0.2.0",
        "react": "18.1.0",
        "react-context-hook": "github:spsgroupab/react-context-hook",
        "react-dom": "18.1.0",
        "react-icons": "4.3.1",
        "react-image-gallery": "1.2.8",
        "react-markdown": "8.0.3",
        "react-responsive-carousel": "3.2.23",
        "react-tender": "1.1.0",
        "sass": "1.51.0",
        "styled-components": "5.3.5",
        "swr": "1.3.0",
        "typescript": "4.6.4"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "12.1.6",
        "@testing-library/jest-dom": "5.16.4",
        "@testing-library/react": "13.2.0",
        "@types/enzyme": "3.10.12",
        "@types/node": "16.11.35",
        "@types/react": "18.0.9",
        "@types/styled-components": "5.1.25",
        "enzyme": "3.11.0",
        "eslint": "8.15.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-react": "7.29.4",
        "eslint-plugin-react-hooks": "4.5.0",
        "jest": "28.1.0",
        "jest-environment-jsdom": "28.1.0",
        "prettier": "2.6.2",
        "webpack": "5.72.1"
    }
}
