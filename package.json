{
    "name": "candybysweden-frontend",
    "version": "1.3.0",
    "private": true,
    "engines": {
        "npm": ">=8",
        "node": ">=16"
    },
    "cacheDirectories": [
        ".next/cache"
    ],
    "scripts": {
        "dev": "next",
        "build": "next build",
        "postbuild": "next-sitemap",
        "analyze": "ANALYZE=true next build",
        "start": "next start -p $PORT",
        "lint": "next lint",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
        "prettier": "prettier --config .prettierrc src/**/*.* --check",
        "prettier-format": "prettier --config .prettierrc src/**/*.* --write"
    },
    "dependencies": {
        "@apollo/client": "3.6.9",
        "@prismicio/client": "6.7.1",
        "@prismicio/helpers": "2.3.4",
        "algoliasearch": "4.14.2",
        "axios": "0.27.2",
        "color": "4.2.3",
        "cookies-next": "2.1.1",
        "cross-env": "7.0.3",
        "destyle.css": "3.0.2",
        "easy-currencies": "1.7.0",
        "graphql": "16.6.0",
        "iso-country-currency": "0.6.0",
        "next": "12.2.5",
        "next-i18next": "12.0.1",
        "next-seo": "5.5.0",
        "node-html-parser": "6.0.0",
        "nprogress": "0.2.0",
        "react": "18.2.0",
        "react-context-hook": "github:filiphsps/react-context-hook",
        "react-dom": "18.2.0",
        "react-icons": "4.4.0",
        "react-rating-stars-component": "2.2.0",
        "react-shopify-analytics": "1.1.0",
        "react-slick": "0.29.0",
        "react-tender": "1.1.1",
        "react-use-cart": "1.13.0",
        "sass": "1.54.9",
        "sharp": "0.31.0",
        "styled-components": "5.3.5",
        "swr": "1.3.0",
        "typescript": "4.8.3",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "12.2.5",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@types/enzyme": "3.10.12",
        "@types/node": "18.7.13",
        "@types/react": "18.0.19",
        "@types/styled-components": "5.1.26",
        "enzyme": "3.11.0",
        "eslint": "8.23.1",
        "eslint-config-next": "12.2.5",
        "eslint-config-prettier": "8.5.0",
        "jest": "29.0.3",
        "jest-environment-jsdom": "29.0.3",
        "next-sitemap": "3.1.22",
        "prettier": "2.7.1",
        "webpack": "5.74.0"
    }
}
