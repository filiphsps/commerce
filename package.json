{
    "name": "sweetsideofsweden-frontend",
    "version": "1.4.0",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "engines": {
        "npm": ">=8",
        "node": ">=18 <=20"
    },
    "cacheDirectories": [
        ".next/cache"
    ],
    "prettier": "@nordcom/prettier",
    "scripts": {
        "prepare": "husky install",
        "start": "next start -p $PORT",
        "dev": "next dev",
        "build": "next build",
        "postbuild": "npm run sitemap",
        "sitemap": "next-sitemap",
        "postsitemap": "for i in public/*.xml; do cat $i | pretty-xml | tee $i > /dev/null; done",
        "lint": "npm-run-all --parallel lint:**",
        "lint:next": "next lint",
        "lint:prettier": "prettier src/**/*.* --check",
        "fix:prettier": "prettier src/**/*.* --write",
        "lint:types": "tsc --project tsconfig.json -noEmit",
        "test": "npm-run-all --parallel test:*",
        "test:jest": "jest --config jest.config.cjs",
        "slicemachine": "start-slicemachine"
    },
    "author": {
        "name": "Nordcom Group Inc.",
        "email": "opensource@nordcom.io",
        "url": "https://nordcom.io/"
    },
    "contributors": [
        {
            "name": "Filiph Siitam SandstrÃ¶m",
            "email": "filiph@nordcom.io",
            "url": "https://github.com/filiphsps/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NordcomInc/sweetsideofsweden-frontend.git"
    },
    "keywords": [
        "nextjs",
        "react"
    ],
    "bugs": {
        "url": "https://github.com/NordcomInc/sweetsideofsweden-frontend/issues"
    },
    "homepage": "https://www.sweetsideofsweden.com/",
    "browserslist": [
        ">0.5%",
        "defaults",
        "supports es6-module",
        "supports css-grid"
    ],
    "dependencies": {
        "@apollo/client": "3.8.8",
        "@highlight-run/next": "7.2.5",
        "@prismicio/client": "7.3.1",
        "@prismicio/next": "1.5.0",
        "@prismicio/react": "2.7.3",
        "@shopify/hydrogen-react": "2023.10.1",
        "@sweetsideofsweden/next-plugin-preval": "3.0.0",
        "@thebeyondgroup/shopify-rich-text-renderer": "1.0.0",
        "accept-language-parser": "1.5.0",
        "color": "4.2.3",
        "convert-units": "2.3.4",
        "critters": "0.0.20",
        "destyle.css": "4.0.0",
        "i18next": "23.6.0",
        "next": "14.0.4",
        "next-i18next": "14.0.3",
        "next-seo": "6.4.0",
        "nprogress": "0.2.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-i18next": "13.3.1",
        "react-icons": "4.11.0",
        "sass": "1.69.5",
        "sharp": "0.32.6",
        "sonner": "1.2.4",
        "styled-components": "6.1.0",
        "swr": "2.2.4"
    },
    "devDependencies": {
        "@nordcom/prettier": "0.1.1",
        "@slicemachine/adapter-next": "0.3.28",
        "@types/accept-language-parser": "1.5.5",
        "@types/color": "3.0.5",
        "@types/convert-units": "2.3.8",
        "@types/jest": "29.5.7",
        "@types/node": "20.8.10",
        "@types/nprogress": "0.2.2",
        "@types/react": "18.2.33",
        "@types/react-dom": "18.2.14",
        "@types/styled-components": "5.1.29",
        "babel-plugin-module-resolver": "5.0.0",
        "eslint": "8.52.0",
        "eslint-config-next": "14.0.2",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-unused-imports": "3.0.0",
        "graphql": "16.8.1",
        "husky": "8.0.3",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "lint-staged": "15.0.2",
        "next-sitemap": "4.2.3",
        "npm-run-all": "4.1.5",
        "prettier": "3.1.0",
        "pretty-xml": "1.2.1",
        "slice-machine-ui": "1.21.2",
        "typescript": "5.3.3"
    },
    "nextBundleAnalysis": {
        "budget": 248400,
        "budgetPercentIncreaseRed": 20,
        "minimumChangeThreshold": 0,
        "showDetails": false
    }
}
