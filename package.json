{
    "name": "candybysweden-frontend",
    "version": "1.3.0",
    "private": true,
    "engines": {
        "npm": ">=8",
        "node": ">=16"
    },
    "cacheDirectories": [
        ".next/cache"
    ],
    "scripts": {
        "dev": "next",
        "build": "next build",
        "postbuild": "next-sitemap",
        "analyze": "ANALYZE=true next build",
        "start": "next start -p $PORT",
        "lint": "next lint",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
        "create-component": "node ./tools/create-component.js",
        "create-slice": "node ./tools/create-slice.js",
        "create-view": "node ./tools/create-view.js",
        "prettier": "prettier --config .prettierrc src/**/*.* --check",
        "prettier-format": "prettier --config .prettierrc src/**/*.* --write"
    },
    "dependencies": {
        "@apollo/client": "3.6.9",
        "@liholiho/react-modal-hook": "3.0.0",
        "@prismicio/client": "6.6.1",
        "@prismicio/helpers": "2.3.1",
        "@sentry/browser": "7.3.1",
        "@sentry/tracing": "7.3.1",
        "axios": "0.27.2",
        "color": "4.2.3",
        "cookies-next": "^2.1.1",
        "countries-list": "2.6.1",
        "country-code-info": "0.3.0",
        "country-code-lookup": "0.0.20",
        "cross-env": "7.0.3",
        "destyle.css": "3.0.2",
        "graphql": "16.5.0",
        "mini-css-extract-plugin": "2.6.1",
        "next": "12.2.0",
        "next-i18next": "11.0.0",
        "next-seo": "5.4.0",
        "nprogress": "0.2.0",
        "react": "18.2.0",
        "react-context-hook": "github:filiphsps/react-context-hook",
        "react-dom": "18.2.0",
        "react-icons": "4.4.0",
        "react-image-gallery": "1.2.8",
        "react-markdown": "8.0.3",
        "react-rating-stars-component": "2.2.0",
        "react-slick": "0.29.0",
        "react-tender": "1.1.0",
        "react-use-cart": "^1.13.0",
        "sass": "1.53.0",
        "sharp": "0.30.7",
        "styled-components": "5.3.5",
        "swr": "1.3.0",
        "typescript": "4.7.4",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "12.2.0",
        "@testing-library/jest-dom": "5.16.4",
        "@testing-library/react": "13.3.0",
        "@types/enzyme": "3.10.12",
        "@types/node": "16.11.41",
        "@types/react": "18.0.14",
        "@types/styled-components": "5.1.25",
        "enzyme": "3.11.0",
        "eslint": "8.18.0",
        "eslint-config-next": "12.2.0",
        "eslint-config-prettier": "8.5.0",
        "jest": "28.1.1",
        "jest-environment-jsdom": "28.1.1",
        "next-sitemap": "3.1.7",
        "prettier": "2.7.1",
        "webpack": "5.73.0"
    }
}
