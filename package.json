{
    "name": "sweetsideofsweden-frontend",
    "version": "1.3.0",
    "private": true,
    "type": "module",
    "engines": {
        "npm": ">=8",
        "node": "18"
    },
    "cacheDirectories": [
        ".next/cache"
    ],
    "scripts": {
        "dev": "npm run hack && next dev",
        "build": "npm run hack && next build",
        "postbuild": "npm run sitemap",
        "sitemap": "next-sitemap && npm run sitemap:pretty",
        "sitemap:pretty": "for i in public/*.xml; do cat $i | pretty-xml | tee $i > /dev/null; done",
        "analyze": "ANALYZE=true next build",
        "start": "npm run hack && next start -p $PORT",
        "lint": "npm run hack && next lint",
        "test": "npm run hack && npm-run-all test:*",
        "test:jest": "jest --coverage",
        "watch:test": "jest --watch",
        "format": "npm-run-all prettier:format:*",
        "prettier": "prettier --config .prettierrc src/**/*.* --check",
        "prettier:format:src": "prettier --config .prettierrc src/**/*.* --write",
        "prettier:format:pages": "prettier --config .prettierrc pages/**/*.* --write",
        "prettier:format:app": "prettier --config .prettierrc app/**/*.* --write",
        "slicemachine": "npm run hack && start-slicemachine",
        "hack": "npm-run-all --sequential hack:*",
        "hack:useclient-directive": "node scripts/useclient-directive-hack.mjs"
    },
    "dependencies": {
        "@apollo/client": "3.7.17",
        "@prismicio/client": "7.1.0",
        "@prismicio/helpers": "2.3.9",
        "@prismicio/next": "1.3.3",
        "@prismicio/react": "2.7.1",
        "@sentry/nextjs": "7.57.0",
        "@shopify/hydrogen": "2023.4.6",
        "@shopify/hydrogen-react": "2023.4.5",
        "accept-language-parser": "1.5.0",
        "algoliasearch": "4.17.2",
        "axios": "1.4.0",
        "color": "4.2.3",
        "destyle.css": "4.0.0",
        "easy-currencies": "1.7.0",
        "fast-average-color": "^9.3.0",
        "graphql": "16.7.1",
        "next": "13.4.8",
        "next-i18next": "14.0.0",
        "next-plugin-preval": "1.2.5",
        "next-seo": "6.1.0",
        "node-html-parser": "6.1.5",
        "nprogress": "0.2.0",
        "react": "18.2.0",
        "react-context-hook": "github:filiphsps/react-context-hook",
        "react-dom": "18.2.0",
        "react-icons": "4.10.1",
        "react-rating-stars-component": "2.2.0",
        "react-shopify-analytics": "1.1.0",
        "react-slick": "0.29.0",
        "react-tender": "1.1.1",
        "rooks": "7.14.1",
        "sass": "1.63.6",
        "sharp": "0.32.1",
        "slick-carousel": "1.8.1",
        "styled-components": "5.3.11",
        "swr": "2.2.0",
        "typescript": "5.1.6",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@slicemachine/adapter-next": "0.3.2",
        "@testing-library/react": "14.0.0",
        "@types/enzyme": "3.10.13",
        "@types/node": "18.16.19",
        "@types/react": "18.2.14",
        "@types/styled-components": "5.1.26",
        "enzyme": "3.11.0",
        "eslint": "8.44.0",
        "eslint-config-next": "13.4.8",
        "eslint-config-prettier": "8.8.0",
        "jest": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "next-sitemap": "4.1.8",
        "npm-run-all": "4.1.5",
        "prettier": "2.8.8",
        "pretty-xml": "1.2.1",
        "slice-machine-ui": "1.4.0"
    }
}
