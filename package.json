{
    "name": "candybysweden-frontend",
    "version": "1.3.0",
    "private": true,
    "type": "module",
    "engines": {
        "npm": ">=8",
        "node": ">=16"
    },
    "cacheDirectories": [
        ".next/cache"
    ],
    "scripts": {
        "dev": "next",
        "build": "next build",
        "postbuild": "next-sitemap",
        "sitemap": "next-sitemap",
        "analyze": "ANALYZE=true next build",
        "start": "next start -p $PORT",
        "lint": "next lint",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
        "prettier": "prettier --config .prettierrc src/**/*.* --check",
        "prettier-format": "prettier --config .prettierrc src/**/*.* --write"
    },
    "dependencies": {
        "@apollo/client": "3.7.14",
        "@prismicio/client": "7.0.1",
        "@prismicio/helpers": "2.3.9",
        "accept-language": "3.0.18",
        "algoliasearch": "4.17.1",
        "axios": "1.4.0",
        "color": "4.2.3",
        "cookies-next": "2.1.1",
        "cross-env": "7.0.3",
        "destyle.css": "4.0.0",
        "easy-currencies": "1.7.0",
        "graphql": "16.6.0",
        "iso-country-currency": "0.7.2",
        "next": "13.4.4",
        "next-i18next": "13.2.2",
        "next-intercept-stdout": "1.0.1",
        "next-seo": "6.0.0",
        "node-html-parser": "6.1.5",
        "nprogress": "0.2.0",
        "react": "18.2.0",
        "react-context-hook": "github:filiphsps/react-context-hook",
        "react-dom": "18.2.0",
        "react-icons": "4.9.0",
        "react-rating-stars-component": "2.2.0",
        "react-shopify-analytics": "1.1.0",
        "react-slick": "0.29.0",
        "react-tender": "1.1.1",
        "react-use-cart": "1.13.0",
        "rooks": "7.14.1",
        "sass": "1.62.1",
        "sharp": "0.32.1",
        "styled-components": "5.3.10",
        "swr": "1.3.0",
        "typescript": "5.0.4",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "14.0.0",
        "@types/enzyme": "3.10.13",
        "@types/node": "18.16.14",
        "@types/react": "18.2.6",
        "@types/styled-components": "5.1.26",
        "enzyme": "3.11.0",
        "eslint": "8.41.0",
        "eslint-config-next": "13.4.4",
        "eslint-config-prettier": "8.8.0",
        "jest": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "next-sitemap": "4.1.3",
        "prettier": "2.8.8",
        "webpack": "5.84.1"
    }
}
