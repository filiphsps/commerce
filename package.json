{
    "name": "candybysweden-frontend",
    "version": "1.3.0",
    "private": true,
    "type": "module",
    "engines": {
        "npm": ">=8",
        "node": ">=16"
    },
    "cacheDirectories": [
        ".next/cache"
    ],
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "postbuild": "next-sitemap",
        "sitemap": "next-sitemap",
        "analyze": "ANALYZE=true next build",
        "start": "next start -p $PORT",
        "lint": "next lint",
        "test": "npm-run-all test:*",
        "test:jest": "jest --coverage",
        "watch:test": "jest --watch",
        "format": "npm-run-all prettier:format:*",
        "prettier": "prettier --config .prettierrc src/**/*.* --check",
        "prettier:format:src": "prettier --config .prettierrc src/**/*.* --write",
        "prettier:format:pages": "prettier --config .prettierrc pages/**/*.* --write",
        "prettier:format:app": "prettier --config .prettierrc app/**/*.* --write",
        "slicemachine": "start-slicemachine"
    },
    "dependencies": {
        "@apollo/client": "3.7.15",
        "@prismicio/client": "7.1.0",
        "@prismicio/helpers": "2.3.9",
        "@prismicio/next": "1.3.1",
        "@prismicio/react": "2.7.0",
        "@sentry/nextjs": "7.55.2",
        "@shopify/hydrogen-react": "2023.4.4",
        "accept-language": "3.0.18",
        "algoliasearch": "4.17.2",
        "axios": "1.4.0",
        "color": "4.2.3",
        "cookies-next": "2.1.2",
        "cross-env": "7.0.3",
        "destyle.css": "4.0.0",
        "easy-currencies": "1.7.0",
        "graphql": "16.6.0",
        "iso-country-currency": "0.7.2",
        "next": "13.4.6",
        "next-i18next": "14.0.0",
        "next-seo": "6.0.0",
        "node-html-parser": "6.1.5",
        "nprogress": "0.2.0",
        "react": "18.2.0",
        "react-context-hook": "github:filiphsps/react-context-hook",
        "react-dom": "18.2.0",
        "react-icons": "4.9.0",
        "react-rating-stars-component": "2.2.0",
        "react-shopify-analytics": "1.1.0",
        "react-slick": "0.29.0",
        "react-tender": "1.1.1",
        "rooks": "7.14.1",
        "sass": "1.63.4",
        "sharp": "0.32.1",
        "styled-components": "5.3.11",
        "swr": "2.1.5",
        "typescript": "5.1.3",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@slicemachine/adapter-next": "0.3.0",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "14.0.0",
        "@types/enzyme": "3.10.13",
        "@types/node": "18.16.18",
        "@types/react": "18.2.12",
        "@types/styled-components": "5.1.26",
        "enzyme": "3.11.0",
        "eslint": "8.42.0",
        "eslint-config-next": "13.4.6",
        "eslint-config-prettier": "8.8.0",
        "jest": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "next-sitemap": "4.1.3",
        "npm-run-all": "4.1.5",
        "prettier": "2.8.8",
        "slice-machine-ui": "1.2.0",
        "webpack": "5.87.0"
    }
}
