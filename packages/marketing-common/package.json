    "$schema": "https://json.schemastore.org/package.json",
    "name": "@nordcom/commerce-marketing-common",
    "version": "0.0.0",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "prettier": "@nordcom/prettier",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.ts",
    "exports": {
        ".": [
            "./src/index.ts",
            "./dist/index.js"
