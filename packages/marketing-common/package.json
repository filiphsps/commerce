        "type": "git",
        "url": "git+https://github.com/NordcomInc/commerce.git"
    },
    "keywords": [],
    "bugs": {
        "url": "https://github.com/NordcomInc/commerce/issues"
    },
    "homepage": "https://shops.nordcom.io/docs/",
    "files": [
        "dist"
    ],
    "dependencies": {
        "@nordcom/nordstar": "0.0.48"
