{
    "extends": ["development"],
    "browserslist": [
        "defaults",
        "not ie 11",
        "not ie <= 10",
        "not and_ff <= 119",
        "not firefox <= 121",
        "not opera <= 104"
    ],
    "hints": {
        "compat-api/css": [
            "default",
            {
                "ignore": [
                    "-webkit-overflow-scrolling",
                    "animation-timeline",
                    "backdrop-filter",
                    "contain-intrinsic-size",
                    "font-size-adjust",
                    "font-smooth",
                    "no-inline-styles",
                    "overscroll-behavior",
                    "scrollbar-color",
                    "scrollbar-width",
                    "text-size-adjust",
                    "text-underline-position",
                    "animation-timeline: scroll(self inline)",
                    "overscroll-behavior-x"
                ]
            }
        ],
        "axe/forms": [
            "default",
            {
                "select-name": "off"
            }
        ],
        "no-inline-styles": "off",
        "typescript-config/is-valid": "off",
        "typescript-config/consistent-casing": "off"
    }
}
