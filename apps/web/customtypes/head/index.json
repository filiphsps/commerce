{
    "id": "head",
    "label": "Header",
    "format": "custom",
    "repeatable": false,
    "status": true,
    "json": {
        "Main": {
            "announcements": {
                "type": "Group",
                "config": {
                    "label": "Announcements",
                    "fields": {
                        "location": {
                            "type": "Select",
                            "config": {
                                "label": "Location",
                                "placeholder": "",
                                "options": ["above", "bellow"],
                                "default_value": "above"
                            }
                        },
                        "background_color": {
                            "type": "Select",
                            "config": {
                                "label": "Background Color",
                                "placeholder": "",
                                "options": ["secondary", "primary"],
                                "default_value": "secondary"
                            }
                        },
                        "content": {
                            "type": "StructuredText",
                            "config": {
                                "label": "Content",
                                "placeholder": "Free shipping on orders above $75",
                                "allowTargetBlank": true,
                                "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
                            }
                        }
                    }
                }
            },
            "navigation": {
                "type": "Group",
                "config": {
                    "label": "Navigation",
                    "fields": {
                        "menu": {
                            "type": "Link",
                            "config": {
                                "label": "Menu",
                                "select": "document",
                                "customtypes": ["menu_item"]
                            }
                        }
                    }
                }
            }
        }
    }
}
