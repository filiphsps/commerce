    "status": true,
    "json": {
        "Main": {
            "announcements": {
                "type": "Group",
                "config": {
                    "label": "Announcements",
                    "fields": {
                        "location": {
                            "type": "Select",
                            "config": {
                                "label": "Location",
                                "placeholder": "",
                                "options": ["above", "bellow"],
                                "default_value": "above"
                            }
                        },
                        "background_color": {
                            "type": "Select",
                            "config": {
                                "label": "Background Color",
                                "placeholder": "",
                                "options": ["secondary", "primary"],
                                "default_value": "secondary"
                            }
                        },
                        "content": {
                            "type": "StructuredText",
                            "config": {
                                "label": "Content",
                                "placeholder": "Free shipping on orders above $75",
                                "allowTargetBlank": true,
                                "single": "paragraph,preformatted,strong,em,hyperlink,embed,rtl"
                            }
                        }
                    }
                }
            }
        }
    }
