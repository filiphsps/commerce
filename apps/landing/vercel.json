{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "public": false,
    "framework": "nextjs",
    "buildCommand": "turbo build",
    "devCommand": "turbo dev",
    "github": {
        "enabled": true,
        "autoJobCancelation": true
    },
    "rewrites": [
        { "source": "/admin/:path*", "destination": "https://nordcom-commerce-admin.vercel.app/admin/:path*" },
        { "source": "/(.*)", "destination": "/" }
    ]
}
