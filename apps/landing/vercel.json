{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "public": false,
    "trailingSlash": true,
    "framework": "nextjs",
    "buildCommand": "turbo build",
    "devCommand": "turbo dev",
    "github": {
        "enabled": true,
        "autoJobCancelation": true
    },
    "rewrites": [
        {
            "source": "/admin/:path*",
            "destination": "https://admin.shops.nordcom.io/admin/:path*"
        }
    ]
}
